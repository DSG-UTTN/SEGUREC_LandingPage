{
    "name": "segurec/landing-page",
    "description": "Landing page corporativa para SEGUREC - Servicios de seguridad privada",
    "version": "1.0.0",
    "type": "project",
    "homepage": "https://segurec.com",
    "authors": [
        {
            "name": "SEGUREC",
            "email": "info@segurec.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Segurec\\": "src/"
        },
        "files": [
            "config/config.php",
            "config/functions.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=PSR12 .",
        "serve": "php -S localhost:8000 -t ."
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "landing-page",
        "php",
        "security",
        "corporate",
        "segurec"
    ],
    "license": "proprietary",
    "support": {
        "email": "soporte@segurec.com",
        "issues": "https://github.com/segurec/landing-page/issues"
    }
}